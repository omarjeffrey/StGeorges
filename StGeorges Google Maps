<script>
function myMap() {
  var mapCanvas = document.getElementById("map");
  var mapOptions = {
    center: new google.maps.LatLng(52.6915, -2.4337),
    zoom: 15
  }
  var map = new google.maps.Map(mapCanvas, mapOptions);
}
</script>

